          <div class="ui link cards">
          <% song.forEach(function(song){ %>

          <div class="card">
          <div class="image">
            <img src="<%=song.url%>">
          </div>
          <div class="content">
            <div class="ui right floated text">
              <div class="meta"><%=song.artistName%></div>
            </div>
            <a class="header"><%=song.songName%></a>
            <div class="meta">
              <span class='data'><%=song.date%></span>
            </div>

            <div class="albumName">
              <%=song.albumName%>
            </div>
          </div>
            <div class="extra content">
              <a>
                <i class="calendar alternate outline icon"></i>
                <%=song.createdAt.toDateString()%>
              </a>

              <span class="right floated">
                <i class="heart outline like icon"></i>
              </span>
            </div>
        </div>

  <% }) %>
  </div>

<br>
<br>

    <div class="ui dividing header">
      <i class="user circle icon"></i>
      Artist DB</div>

      <!--<div class ="ui four column equal width center padded grid">-->
        <div class="ui five column stackable padded middle aligned centered color grid">
          <% for(var i = 0; i < song.length; i++) { %>

            <%if(i==0) {%>
          <div class="red column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==1) {%>
          <div class="orange column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==2) {%>
          <div class="yellow column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==3) {%>
          <div class="green column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==4) {%>
          <div class="blue column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==5) {%>
          <div class="teal column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==6) {%>
          <div class="blue column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==7) {%>
          <div class="violet column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==8) {%>
          <div class="purple column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==9) {%>
          <div class="pink column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==10) {%>
          <div class="brown column"><%=song[i].artistName %></div>
            <%}%>
            <%if(i==11) {%>
          <div class="violet column"><%=song[i].artistName %></div>
            <%}%>

        <% } %>
      </div>

      <br>
      <br>

    <div class="ui dividing header">
      <i class="headphones icon"></i>
      Album DB</div>

      <div class ="ui four column equal width center padded grid">
        <% song.forEach(function(album){ %>
          <div class="column">
          <h3 class="ui center aligned segment"><%=album.albumName%></h3>
          </div>
        <% }) %>
      </div>
</div>
  </div>
